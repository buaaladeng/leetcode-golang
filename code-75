//思路，0往左边移动，2往右边移动，1跳过
func sortColors(nums []int)  {
	n := len(nums)
	if n == 0{
		return
	}
	zero := 0
	two := n-1
	for i:=0; i <= two;{
		if nums[i] == 0{
			nums[i],nums[zero] = nums[zero],nums[i]
			zero++
			i++
		}else if nums[i] == 2{
			nums[i],nums[two] = nums[two],nums[i]
			two--
		}else {
			i++
		}
	}
}
