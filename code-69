//传统方法
func mySqrt(x int) int {
	if x == 0 || x == 1{
		return x
	}
	i:=1
	for ;i<=x/2;i++{
		if i*i>x{
			break
		}else if i*i == x{
			return i
		}
	}
	return i-1
}

//2分法
func mySqrt(x int) int {
	if x == 0 || x == 1{
		return x
	}
	start := 1
	end := x
	for start<=end{
		mid := start+(end-start)/2
		if mid*mid == x{
			return mid
		}else if mid*mid <x{
			start = mid+1
		}else {
			end = mid-1
		}
	}
	return start-1
}
