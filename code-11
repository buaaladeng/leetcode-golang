func maxArea(height []int) int {
	n := len(height)
	start := 0
	end := n-1
	res := 0

	for start<end{
		min := minInt(height[start],height[end])
		res = maxInt(res,min * (end-start))
		if height[start]<height[end]{
			start++
		}else {
			end--
		}
	}
	return res

}

func maxInt(a,b int)int{
	if a>b{
		return a
	}else {
		return b
	}
}

func minInt(a,b int)int{
	if a>b{
		return b
	}else {
		return a
	}
}
