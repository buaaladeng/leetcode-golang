func maxSubArray(nums []int) int {
	n:= len(nums)
	res := nums[0]
	sum := 0
	for i:=0;i<n;i++{
		if sum >= 0{
			sum += nums[i]
		}else {
			sum = nums[i]
		}
		res = maxInt(res,sum)
	}
	return res
}

func maxSubArray(nums []int) int {
	n := len(nums)
	if n == 0{
		return 0
	}
	res := nums[0]
	for i:=1;i<n;i++{
		if nums[i-1]>0{
			nums[i] += nums[i-1]
		}
		res = maxInt(res,nums[i])
	}
	return res
}


func maxInt(a,b int)int{
	if a>b{
		return a
	}else {
		return b
	}
}
