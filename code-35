func searchInsert(nums []int, target int) int {
	n := len(nums)
	l := 0
	r := n -1
	for l <= r{
		mid := (l+r)/2
		if nums[mid] == target{
			return mid
		}else if nums[mid]< target{
			l = mid +1
		}else {
			r = mid -1
		}
	}
	return l
}


func searchInsert(nums []int, target int) int {
	n := len(nums)
	for i:=0;i<n;i++{
		if target<=nums[i]{
			return i
		}
	}
	return n
}
