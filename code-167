func twoSum(numbers []int, target int) []int {
	n := len(numbers)
	res := make([]int,0)
	if n < 2 {
		return res
	}
	l := 0 
	r := n -1
	
	for l < r {
		if numbers[l] + numbers[r] == target {
			res = append(res,l+1)
			res = append(res,r+1)
			break
		} else if numbers[l] + numbers[r] < target{
			l++
		}else {
			r--
		}
		
	}
	return res
}
