func simplifyPath(path string) string {
	n := len(path)
	stack := make([]string,0)
	tmp := make([]byte,0)
	for i:=0;i<n;i++ {
		tmp = tmp[:0]
		for i < n && path[i] != '/' {
			tmp = append(tmp, path[i])
			i++
		}
		s := string(tmp)
		switch s {
		case "", ".":
		case "..":
			if len(stack)>0{
			stack = stack[:len(stack)-1]}
		default:
			stack = append(stack,s)
		}
	}
	return "/" + strings.Join(stack,"/")
}
