func removeDuplicates(nums []int) int {
	n :=len(nums)
	if n == 0{
		return 0
	}
	start :=0
	end := 1
	num := 0
	for end<n{
		if nums[start]!=nums[end]{
			start++
			nums[start] = nums[end]
			num =0
		}else {
			if num <2{
				start++
				nums[start] = nums[end]
				num++
			}
		}
		end++
	}
	return start+1
}


func removeDuplicates(nums []int) int {
	n := len(nums)
	res := 2
	for j:=res;j<n;j++{
		if nums[res-2]!=nums[j]{
			nums[res] = nums[j]
			res++
		}
	}
	return res
}
