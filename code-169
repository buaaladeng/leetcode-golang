// 时间复杂度 n
func majorityElement(nums []int) int {
	num := 1
	res := nums[0]
	for i:=1;i<len(nums);i++{
		if nums[i] == res {
			num++
		}else {
			num--
		}
		if num == 0{
			num = 1
			res = nums[i]
		}
	}
	return res
}

// 时间复杂度 log n

func majorityElement(nums []int) int {
	sort.Ints(nums)
	return nums[len(nums)/2]

}


