func maxProfit(prices []int) int {
	n := len(prices)
	if n == 0{
		return 0
	}
	min := math.MaxInt32
	res := 0
	for i:=0;i<n;i++{
		min = minInt(min,prices[i])
		res = maxInt(res,prices[i]-min)
	}
	return res
}

func maxInt(a,b int) int{
	if a<b{
		a = b
	}
	return a
}

func minInt(a,b int) int{
	if a>b{
		a = b
	}
	return a
}
