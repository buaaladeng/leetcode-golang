func firstMissingPositive(nums []int) int {
	n := len(nums)
	if n == 0{
		return 1
	}
	for i:=0;i<n;i++{
		for nums[i] - 1 >= 0 && nums[i] - 1 < n && nums[nums[i] - 1] != nums[i]{
			fmt.Println("swap:" + strconv.Itoa(nums[i]) + "and" + strconv.Itoa(nums[nums[i]-1]))
			nums[i],nums[nums[i]-1] = nums[nums[i]-1],nums[i]
		}
	}
	fmt.Println(nums)
	for i:=0;i<n;i++{
		if nums[i]-1!=i{
			return i+1
		}
	}
	return n+1
}
