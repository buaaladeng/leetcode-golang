func convert(s string, numRows int) string {
	res := ""
	n:=len(s)
	if n == 0 || numRows == 0{
		return ""
	}
	if numRows == 1{
		return s
	}
	array := make([][]byte,0)
	for i:=0;i<numRows;i++{
		array  = append(array, make([]byte,0))
	}
	row := 0
	flag := 1
	for i:=0;i<n;i++{
		array[row] = append(array[row], s[i])
		row += flag
		if row == numRows  || row == -1{
			flag = -flag
			row += flag
			row += flag
		}
	}

	for i:=0;i<numRows;i++{
		res += string(array[i])
	}
	return res
}
