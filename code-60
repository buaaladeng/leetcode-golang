
func getPermutation(n int, k int) string {
	if n == 0 || k == 0{
		return ""
	}
	base :=1
	for i:=1;i<n;i++{
		base *= i
	}
	res :=make([]byte,n)
	num := make([]byte,n)
	for i:=0;i<n;i++{
		num[i] = byte(i) + '1'
	}
	k--
	for i:=1;i<n;i++{
		index := k/base
		k %= base
		res[i-1] = num[index]
		num = append(num[:index],num[index+1:]...)
		base /=(n-i)
	}
	res[n-1] = num[0]
	return string(res)
}
