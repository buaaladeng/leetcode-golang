func isPalindrome(head *ListNode) bool {
	res := make([]int,0)
	for head!=nil{
		res = append(res,head.Val)
		head = head.Next
	}
	start := 0
	end := len(res)-1
	for start<end{
		if res[start]!=res[end]{
			return false
		}else {
			start++
			end--
		}
	}
	return true
}

func isPalindrome(head *ListNode) bool {
	fast := head
	slow := head
    if head == nil{
        return true
    }
	for fast!=nil && fast.Next!=nil{
		slow = slow.Next
		fast = fast.Next.Next
	}
    fmt.Println(slow.Val)
	fast = reverseList(slow)
	for head!=slow{
		if fast.Val != head.Val{
			return false
		}else {
			fast = fast.Next
			head=head.Next
		}
	}
	return true
}

func reverseList(head *ListNode) *ListNode {
	if head == nil || head.Next == nil{
		return head
	}
	p := reverseList(head.Next)
	head.Next.Next = head
	head.Next = nil
	return p
}

